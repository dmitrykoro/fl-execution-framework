[run]
source = src
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    config.py
    config-3.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    @abstractmethod

show_missing = True

[html]
directory = htmlcov