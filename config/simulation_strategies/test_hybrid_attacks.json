{
  "shared_settings": {
    "aggregation_strategy_keyword": "krum",
    "num_krum_selections": 6,
    "remove_clients": "true",
    "begin_removing_from_round": 2,
    "dataset_keyword": "femnist_iid",
    "model_type": "cnn",
    "use_llm": "false",
    "num_of_rounds": 15,
    "num_of_clients": 10,
    "num_of_malicious_clients": 3,
    "show_plots": "false",
    "save_plots": "true",
    "save_csv": "true",
    "preserve_dataset": "false",
    "training_subset_fraction": 0.5,
    "training_device": "cpu",
    "cpus_per_client": 2,
    "gpus_per_client": 0.0,
    "min_fit_clients": 10,
    "min_evaluate_clients": 10,
    "min_available_clients": 10,
    "evaluate_metrics_aggregation_fn": "weighted_average",
    "num_of_client_epochs": 2,
    "batch_size": 32
  },
  "simulation_strategies": [
    {
      "attack_type": "hybrid_attack",
      "dynamic_attacks": {
        "enabled": true,
        "schedule": [
          {
            "start_round": 3,
            "end_round": 12,
            "selection_strategy": "specific",
            "client_ids": [0, 1, 2],
            "attack_config": {
              "type": "label_flipping",
              "params": {
                "flip_fraction": 0.6,
                "num_classes": 62
