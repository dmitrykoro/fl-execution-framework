            }
          },
          {
            "start_round": 5,
            "end_round": 10,
            "selection_strategy": "specific",
            "client_ids": [0, 1],
            "attack_config": {
              "type": "gaussian_noise",
              "params": {
                "mean": 0.0,
                "std": 0.15
              }
            }
          },
          {
            "start_round": 7,
            "end_round": 15,
            "selection_strategy": "specific",
            "client_ids": [0],
            "attack_config": {
              "type": "brightness",
              "params": {
                "factor": 0.4
              }
            }
          }
        ]
      }
    }
  ]
}
